<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://JAVA.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<servlet>
	  <servlet-name>login</servlet-name>
	  <servlet-class>team.cl2y2x.practicesys.controlservlet.LoginServlet</servlet-class>   <!--Servlet的类-->
	</servlet>
	<servlet-mapping>
	  <servlet-name>login</servlet-name>               
	  <url-pattern>/login</url-pattern>         					    <!--映射的url路径 -->	
	</servlet-mapping>
	<servlet>
	  <servlet-name>exercise</servlet-name>
	  <servlet-class>team.cl2y2x.practicesys.controlservlet.ExerciseServlet</servlet-class>   <!--Servlet的类-->
	</servlet>
	<servlet-mapping>
	  <servlet-name>exercise</servlet-name>               
	  <url-pattern>/exercise</url-pattern>         					    <!--映射的url路径 -->	
	</servlet-mapping>
	<servlet>
	  <servlet-name>detail</servlet-name>
	  <servlet-class>team.cl2y2x.practicesys.controlservlet.DetailServlet</servlet-class>   <!--Servlet的类-->
	</servlet>
	<servlet-mapping>
	  <servlet-name>detail</servlet-name>               
	  <url-pattern>/detail</url-pattern>         					    <!--映射的url路径 -->	
	</servlet-mapping>
	<servlet>
	  <servlet-name>merge</servlet-name>
	  <servlet-class>team.cl2y2x.practicesys.controlservlet.MergeServlet</servlet-class>   <!--Servlet的类-->
	</servlet>
	<servlet-mapping>
	  <servlet-name>merge</servlet-name>               
	  <url-pattern>/merge</url-pattern>         					    <!--映射的url路径 -->	
	</servlet-mapping>
	<servlet>
	  <servlet-name>mistake</servlet-name>
	  <servlet-class>team.cl2y2x.practicesys.controlservlet.MistakeServlet</servlet-class>   <!--Servlet的类-->
	</servlet>
	<servlet-mapping>
	  <servlet-name>mistake</servlet-name>               
	  <url-pattern>/mistake</url-pattern>         					    <!--映射的url路径 -->	
	</servlet-mapping>
	<servlet>
	  <servlet-name>search</servlet-name>
	  <servlet-class>team.cl2y2x.practicesys.controlservlet.SearchServlet</servlet-class>   <!--Servlet的类-->
	</servlet>
	<servlet-mapping>
	  <servlet-name>search</servlet-name>               
	  <url-pattern>/search</url-pattern> 								<!--     映射的url路径 -->	   					    
	</servlet-mapping>
	<servlet>
	  <servlet-name>studyinfo</servlet-name>
	  <servlet-class>team.cl2y2x.practicesys.controlservlet.StudyInfoServlet</servlet-class>   <!--Servlet的类-->
	</servlet>
	<servlet-mapping>
	  <servlet-name>studyinfo</servlet-name>               
	  <url-pattern>/studyinfo</url-pattern> 								<!--     映射的url路径 -->	   					    
	</servlet-mapping>
	<jsp-config>
		<taglib>
	    	<taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
	   		<taglib-location>/WEB-INF/c.tld</taglib-location>
	    </taglib>
    </jsp-config>
</web-app>
